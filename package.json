{
  "name": "bulk-message-sender",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "web:up": "docker compose -f docker-compose.yml up -d",
    "web:logs": "docker compose -f docker-compose.yml logs -f web",
    "web:down": "docker compose -f docker-compose.yml down",
    "web:restart": "npm run web:down && npm run web:up",
    "db:up": "docker compose -f docker-compose.dev-db.yml up -d",
    "db:logs": "docker compose -f docker-compose.dev-db.yml logs -f db",
    "db:down": "docker compose -f docker-compose.dev-db.yml down",
    "db:restart": "npm run db:down && npm run db:up"
  },
  "dependencies": {
    "@prisma/client": "5.16.1",
    "bcryptjs": "2.4.3",
    "csv-parse": "5.5.6",
    "libphonenumber-js": "1.10.61",
    "p-queue": "8.0.1",
    "next": "14.2.5",
    "next-auth": "4.24.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "twilio": "4.20.1",
    "zod": "3.23.8",
    "react-aria-components": "1.3.0",
    "nuqs": "1.17.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/node": "20.11.30",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "prisma": "5.16.1",
    "typescript": "5.4.5",
    "ts-node": "10.9.2",
    "tailwindcss": "3.4.9",
    "postcss": "8.4.41",
    "autoprefixer": "10.4.20",
    "vitest": "1.6.0",
    "@testing-library/react": "14.3.1",
    "@testing-library/jest-dom": "6.4.6",
    "jsdom": "24.1.0",
    "prettier": "3.3.3"
  }
}
